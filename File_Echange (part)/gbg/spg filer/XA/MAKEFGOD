.AUTODEPEND

#		*Translator Definitions*
CC = c:\xa\bin\xac
ASM = c:\xa\bin\xac
LINK = c:\xa\bin\xac -A0,20,1E0 -BIN


#		*Implicit Rules*
.c.obj:
  $(CC)  $< -O -Zg -C

.as.obj:
   $(ASM) $< -O -Zg -C -BM

#		*Explicit Rules*

ud.b: sou.obj hint.obj tab.obj
      $(LINK) sou.obj hint.obj tab.obj


#		*Individual File Dependencies*
sou.obj:	sou.c vars.c
tab.obj:	tab.c vars.c
hint.obj:	hint.as
