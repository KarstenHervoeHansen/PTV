  MENUHELP = "Set options for the XA CrossView Pro Debugger"

  TAB
  {
    TOOLID = XVW
    NAME = "Settings"

    BLOCK
    {
      NAME = "Execution environment"

      OPTION
      {
        NAME  = "The ROM monitor debugger is available as a separate package."
        ID    = XvwLanguageText1
        TYPE  = TEXT
