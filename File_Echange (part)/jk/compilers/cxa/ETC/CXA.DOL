TOOLCHAIN
{
  DOILVERSION  = 1.0
  TITLE        = "TASKING C for XA 2.0 r2"
  ABBREV       = "XA"

  BINDIR       = "\\bin"		// do not use Capitals for DIR's
  INCDIR       = "\\include"
  LIBDIR       = "\\lib\\xa"
  OBJSUFFIX    = .obj
  ERRORPARSER1 = "/^%e[EWSFR][ \\t]+[0-9]+:[ \\t]*%m.*/%backward %save/^%f[^ \\t][^:][^ \\t]+:/%restore %save/^[ \\t]+%l[0-9]+:/%restore"
  ERRORPARSER2 = "/asxa %e[EWSF][0-9]+: %f[^ ]+ line %l[0-9]+.*: %m.*/"
  ERRORPARSER3 = "/^\\*\\*\\*\\* %e[EW][^\\(]+\\(%f[^, ]+, line #%l[0-9]+\\) %m.*/"
}

