MENU
{
  MENUID   = M
  NAME	   = "&Processor Options"
  TITLE    = "Processor Options" 
  MENUHELP = "Select CPU and set properties"
  NOPREVIEW				// This menu has FLAGTEXT only

  TAB
  {
    TOOLID = MODELXA
    NAME = "Processor"

    BLOCK
    {
      NAME = "CPU derivative for SFRs"

      OPTION
      {
        NAME = "&CPU (Special Function Registers):"
        ID = Cpu
        TYPE = POPUPLIST
        STATE = CpuXaG3
        ITEM
        {
          NAME = "Philips XA-G1"
          ID = CpuXaG1
          FLAGTEXT = "xag1"
        }
        ITEM
        {
          NAME = "Philips XA-G2"
          ID = CpuXaG2
          FLAGTEXT = "xag2"
        }
        ITEM
        {
          NAME = "Philips XA-G3"
          ID = CpuXaG3
          FLAGTEXT = "xag3"
        }









        ITEM
        {
          NAME = "Philips XA-S3"
          ID = CpuXaS3
          FLAGTEXT = "xas3"
        }
        ITEM
        {
          NAME = "Philips XA-SCC"
          ID = CpuXaSCC
          FLAGTEXT = "xascc"
        }
        ITEM
        {
          NAME = "Philips Artistic"
          ID = CpuXaArt
          FLAGTEXT = "xaart"
        }
        ITEM
        {
          NAME = "User Specified"
          ID = CpuUser
        }
        ITEM
        {
          NAME = "None"
          ID = CpuNone
        }
      }

      EMPTYLINE

      OPTION
      {
        NAME = "You can specify your own CPU name and create SFR files using the"
        ID = CpuUserText1
	TYPE = TEXT
      }
      OPTION
      {
	NAME = "REG<name>.<EXT> naming convention: .SFR (C compiler),"
        ID = CpuUserText2
	TYPE = TEXT
      }
      OPTION
      {
	NAME = ".SFR( Assembler), .DEF (Register Manager), .DAT (CrossView Pro)"
        ID = CpuUserText3
	TYPE = TEXT
      }

      EMPTYLINE

      OPTION
      {
        NAME = "&User specified CPU name:"
        ID = CpuUserName
        PARENT = CpuUser
        TYPE = EDITBOX
        SELECTED
        {
          FLAGTEXT = "%s"
        }
      }
    }
  }

  TAB
  {
    TOOLID = MODELXA
    NAME = "Memory"

    BLOCK
    {
      NAME = "CPU memory specification"

      OPTION
      {
        NAME = "On-chip EPROM/ROM program memory size:"
        ID = IRomSize
        TYPE = POPUPLIST
        STATE = IRom32k
        ITEM
        {
          NAME = "8 Kbytes"
          ID = IRom8k
	  PARENT = CpuXaG1 | CpuUser | CpuNone
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _ICODE 0x2000\n"
        }
        ITEM
        {
          NAME = "16 Kbytes"
          ID = IRom16k
	  PARENT = CpuXaG2 | CpuUser | CpuNone
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _ICODE 0x4000\n"
        }
        ITEM
        {
          NAME = "32 Kbytes"
          ID = IRom32k
	  PARENT = CpuXaG3 | CpuXaS3 | CpuXaArt | CpuUser | CpuNone
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _ICODE 0x8000\n"
        }
        ITEM
        {
          NAME = "No on-chip program memory"
          ID = IRom0
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _ICODE 0x0\n"
        }
        ITEM
        {
          NAME = "User Specified"
          ID = IRomUser
	  PARENT = CpuUser | CpuNone
        }
      }

      OPTION
      {
        NAME = "On-chip data R&AM size:"
        ID = IRamSize
        TYPE = POPUPLIST
        STATE = IRam1k
        ITEM
        {
          NAME = "256 bytes"
          ID = IRam256
  	  PARENT = CpuXaSCC | CpuUser | CpuNone 
  	  DEFMACRO = "FILE=%p.i;LK,LC;#define _IRAM 0x100\n"
        }
        ITEM
        {
          NAME = "512 bytes"
          ID = IRam512
	  PARENT = CpuXaG1 | CpuXaG2 | CpuXaG3 | CpuXaArt | CpuUser | CpuNone
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _IRAM 0x200\n"
        }
        ITEM
        {
          NAME = "1 Kbytes"
          ID = IRam1k
	  PARENT = CpuXaS3 | CpuUser | CpuNone
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _IRAM 0x400\n"
        }
        ITEM
        {
          NAME = "User Specified"
          ID = IRamUser
	  PARENT = CpuUser
        }
      }

      EMPTYLINE

      OPTION
      {
        NAME = "&User specified on-chip EPROM/ROM program memory size (0-0FFFFH):"
        ID = IRomUserSize
        PARENT = IRomUser
        TYPE = EDITBOX WIDE
        SELECTED
        {
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _ICODE %s\n"
        }
      }

      OPTION
      {
        NAME = "U&ser specified on-chip data RAM size (0-0FFFFH):"
        ID = IRamUserSize
        PARENT = IRamUser
        TYPE = EDITBOX WIDE
        SELECTED
        {
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _IRAM %s\n"
        }
      }

      EMPTYLINE

      OPTION
      {
        NAME = "&Single chip solution (no external memory)"
        ID = CpuSingleChip
        PARENT = !CpuXaSCC
        TYPE = RADIOBUTTON
        STATE = NOTSELECTED
      }

      OPTION
      {
        NAME = "&External memory (specify ROM/RAM areas in Linker Options...)"
        ID = CpuExtMem
        TYPE = RADIOBUTTON
        STATE = SELECTED
      }

      EMPTYLINE

      OPTION
      {
        NAME = "&Separate external code and data bus (Harvard architecture)"
        ID = Harvard
        PARENT = !CpuXaSCC
        TYPE = RADIOBUTTON
        STATE = SELECTED
      }

      OPTION
      {
        NAME = "&Unified external code and data bus"
        ID = Unified
        PARENT = CpuExtMem
        TYPE = RADIOBUTTON
        STATE = NOTSELECTED
        SELECTED
        {
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _UNIFIED_BUS 1\n"
        }
      }

//
// EDE include invisible options
//
      OPTION
      {
        ID = EdeIncludeSfr1
	PARENT = !CpuUser & !CpuNone
        TYPE = INVISIBLE
        STATE = SELECTED
	SELECTED
	{
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _REGSFR reg$(Cpu).dat\n"
	}
      }
      OPTION
      {
        ID = EdeIncludeSfr2
	PARENT = CpuUser & !CpuNone
        TYPE = INVISIBLE
        STATE = SELECTED
	SELECTED
	{
	  DEFMACRO = "FILE=%p.i;LK,LC;#define _REGSFR reg$(CpuUserName).dat\n"
	}
      }
      OPTION
      {
        ID = EdeIncludeExtMem
	PARENT = LcExtCode | LcExtCodeLin | LcExtRam | LcExtRamLin | LcExtRom
        TYPE = INVISIBLE
        STATE = SELECTED
        SELECTED
        {
          DEFMACRO = "FILE=%p.i;LK,LC;#define _XMEM 1\n"
        }
      }
      OPTION
      {  
        ID = EdeIncludeNoExtMem
        PARENT = !(LcExtCode | LcExtCodeLin | LcExtRam | LcExtRamLin | LcExtRom)
        TYPE = INVISIBLE
        STATE = SELECTED
        SELECTED
        { 
          DEFMACRO = "FILE=%p.i;LK,LC;#define _XMEM 0\n"
        }
      }
    }  
  }
}       

