TOOL
{
  TOOLID = LK
  MANUALID = AM
  SUFFIXPAIR = .obj.out
  ACTION = "%exe(ccxa.exe) -Wlk -emEDE=\\\"$(PROJ).i\\\" -o $@ -f<<EOF
  $(separate \"\\n\" $(match .obj $!))
  $(separate \"\\n\" $(match .a $!)) -cl %opt
  EOF"
}
TOOL
{
  TOOLID = LK
  MANUALID = AM
  SUFFIXPAIR = .a.out
  ACTION = ""		// see .obj.out action
}

