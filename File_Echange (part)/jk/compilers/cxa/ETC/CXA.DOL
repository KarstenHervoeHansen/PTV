        {
          OPTIONTEXT = "-tcfg sim.cfg"
        }
      }

      OPTION
      {
        NAME = "ROM monitor"
        ID = XvwRom
        TYPE = RADIOBUTTON
        STATE = NOTSELECTED 
        SELECTED
        {
          OPTIONTEXT = "-tcfg rism.cfg -D $(XvwDeviceType)$(XvwSerialPort)$(XvwBaud)$(XvwHost)$(XvwPort)"
