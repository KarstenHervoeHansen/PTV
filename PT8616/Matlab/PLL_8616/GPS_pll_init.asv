clear;

Ga = 1/2;
Gb = 1/128;
Kv=4e-6;
Ts=1/100e6; %Clockcycle for phasedetector

loopfilter = ss(0,1,Ga,Gb);

Gdac = 2^-16*5;

ppsgenerator = Kv;

phasedifference = ss(0,1,1/Ts,0)*exp(-i*pi*);

loopgain = phasedifference*loopfilter*Gdac*ppsgenerator;

figure(1)
bode(loopfilter)
title('Loopfilter')
grid on
figure(2)
bode(phasedifference)
title('Phase difference')
grid on
figure(3)
bode(loopgain)
title('Loopgain')
grid on

