SUBDESIGN uvblnlsb
(
	clk,ab[1..0],sb[2..0],enable:	INPUT;
	cout[1..0]:						OUTPUT;
)
VARIABLE
	dout[1..0],cout[1..0]:			DFFE;

BEGIN
	cout[].d 	= dout[1..0].q;
	cout[].clk	= clk;
%	cout[].ena	= enable;%

	dout[].clk	= clk;
	dout[].ena	= !enable;%Til linie 23 og linie 623 + burst-gate%

	IF sb[] == 0 THEN
	 dout[].d	= ab[];
	ELSE
	 dout[].d 	= H"0";
	END IF;
END;

