SUBDESIGN 4dffs
(
	clk,s3,s2,s1,s0:				INPUT;
	s3b,s2b,s1b,s0b:				OUTPUT;
)
VARIABLE
	sa,sb,sc,sd:					DFF;

BEGIN
	sa.clk	= clk;
	sb.clk	= clk;
	sc.clk	= clk;
	sd.clk	= clk;
	sa.d	= s3;
	sb.d	= s2;
	sc.d	= s1;
	sd.d	= s0;
	s3b 	= sa.q;
	s2b 	= sb.q;
	s1b 	= sc.q;
	s0b 	= sd.q;
END;

