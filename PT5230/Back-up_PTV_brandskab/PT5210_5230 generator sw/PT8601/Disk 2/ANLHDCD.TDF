SUBDESIGN anlhdcd
(
	clk,syssel,da[9..0]:		INPUT;
	upint,genst:				OUTPUT;
)
VARIABLE
	hint:					DFF;

BEGIN
	hint.clk	= genst;
	hint.clrn	= !da7;
	hint.d		= vcc;
IF syssel THEN
	genst = (da[9..0]==857);%NTSC%
ELSE
	genst = (da[9..0]==863);%PAL-G%
END IF;
	upint	= !hint.q;
END;

