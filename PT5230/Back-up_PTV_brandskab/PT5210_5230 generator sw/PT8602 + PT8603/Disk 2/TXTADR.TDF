SUBDESIGN txtadr
(
	clk, s0, s1,a0:				INPUT;
	txtblk[1..0],txtnr[3..0]:	OUTPUT;
)

VARIABLE
	txtnr[3..0]:	DFF;
%	txtblk[1..0]:	DFF;%

BEGIN
	txtnr[3..0].clk  = clk;

	IF (s0 # s1) # (txtnr[3..0]) > 0 THEN
      txtnr[3..0].d = txtnr[3..0].q+1;
	END IF;

	txtblk0	= s0;
	txtblk1	= a0%s1%;

END;


