SUBDESIGN count8
(
	clk,start:				INPUT;
	sb[2..0]:				OUTPUT;
)
VARIABLE
	sb[2..0]:				DFFE;
	st:						DFF;
BEGIN
	st.d = start;
	st.clk = clk;

	sb[2..0].clk= clk;
	sb[].ena  = !(sb[]==7);
	sb[].clrn = st;
	IF st  == 1 THEN
 	 sb[].d	 = sb[].q+1;
%	ELSE 
	 sb[].d	 =H"0"; %
	END IF;
END;

