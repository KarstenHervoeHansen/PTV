SUBDESIGN hdecod1
(
	clk,da[10..0]:			INPUT;
	hload,upint:	 		OUTPUT;
)
VARIABLE
	hload:				DFF;
	hint:				DFFE;

BEGIN

	hint.clk	= clk;
	hint.d		= VCC; 
	hint.ena	= (da[10..0] == 1392);
	hint.clrn	= !(da[10..0] == 1486);
	
	upint 		= !hint;

	hload.clk	= clk;
	hload.d		= (da[10..0]==1439);

END;

