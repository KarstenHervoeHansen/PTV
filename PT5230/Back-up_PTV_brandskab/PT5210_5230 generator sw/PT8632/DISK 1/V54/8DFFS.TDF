	d0,d1,d2,d3,d4,d5,d6,d7:	DFF;

BEGIN
	d7.clk	= clk;
	d7.d	= ab7;
	d6.clk	= clk;
	d6.d	= ab6;
	d5.clk	= clk;
	d5.d	= ab5;
	d4.clk	= clk;
	d4.d	= ab4;
	d3.clk	= clk;
	d3.d	= ab3;
	d2.clk	= clk;
	d2.d	= ab2;
	d1.clk	= clk;
	d1.d	= ab1;
	d0.clk	= clk;
	d0.d	= ab0;
END;

