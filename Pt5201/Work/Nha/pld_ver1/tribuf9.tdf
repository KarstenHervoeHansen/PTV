SUBDESIGN tribuf9
(
	oe,d9,d8,d7,d6,d5,d4,d3,d2,d1,d0:				INPUT;
	dmbuf[9..0]:				OUTPUT;
)
VARIABLE
	tnode[9..0] :	TRI_STATE_NODE;
BEGIN
	tnode9	= TRI(d9,!oe);
	tnode8	= TRI(d8,!oe);
	tnode7	= TRI(d7,!oe);
	tnode6	= TRI(d6,!oe);
	tnode5	= TRI(d5,!oe);
	tnode4	= TRI(d4,!oe);
	tnode3	= TRI(d3,!oe);
	tnode2	= TRI(d2,!oe);
	tnode1	= TRI(d1,!oe);
	tnode0	= TRI(d0,!oe);

	dmbuf[9..0]		= tnode[9..0];

END;

