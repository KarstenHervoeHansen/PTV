|"palasm" pins:(bus[7..0],load1,load2,load3,line,clock,busselect,ud[10..0],g,pedoff),
|         nodes:(subuf[10..0],libuf[7..0],subufb[10..0],libufb[7..0])



|conditioning: dff(subuf[7..0],load1)
|conditioning: dff(subuf[10..8],load2)
|conditioning: dff(libuf[7..0],load3)

|subuf[7..0]  = bus[7..0]
|subuf[10..8] = bus[2..0]
|libuf[7..0]  = bus[7..0]


|conditioning: dff(G,load2)
|G         = bus7

|conditioning: dff(PEDOFF,load2)
|pedoff = bus6


|conditioning: dff(subufb[10..0],clock)
|conditioning: dff(libufb[7..0],clock)

|subufb[10..0]  =( subuf[10..0] & line  )
|               #( subufb[10..0] & line')

|libufb[7..0]  =( libuf[7..0] & line  )
|               #( libufb[7..0] & line')

|libufb[10..8]=0

|conditioning: dff(ud[10..0],clock)
|ud[10..0] = (busselect  & libufb[10..0]) 
|          # (busselect' & subufb[10..0])

